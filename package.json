{
    "name": "mask-bootstrap-ui",
    "version": "0.0.1",
    "dependencies": {
        "maskjs": "latest"
    },
    "devDependencies": {
        "atma-loader-babel": "latest"
    },
    "scripts": {
        "test": "atma test",
        "build": "atma"
    },
    "atma": {
        "settings": {
            "atma-loader-babel": {
                "extensions": [
                    "test",
                    "es6"
                ]
            }
        },
        "plugins": [
            "atma-loader-babel"
        ]
    }
}